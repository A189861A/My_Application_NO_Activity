<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android">

    <permission
        android:name="com.example.app2.PERMISSION_ACCESS_USER_DATA"
        android:protectionLevel="normal" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.My_Application_NO_Activity">
        <service
            android:name=".MyService"
            android:enabled="true"
            android:exported="true"></service>

        <activity
            android:name=".MainActivity3"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".MainActivity2"
            android:exported="true"></activity>
        <activity
            android:name=".MainActivityUser"
            android:exported="true" />

        <provider
            android:name=".UserContentProvider"
            android:authorities="com.example.app2.userprovider"
            android:enabled="true"
            android:exported="true"
            android:permission="com.example.app2.PERMISSION_ACCESS_USER_DATA" />

        <activity
            android:name=".MainActivity"
            android:exported="true" />
    </application>

</manifest>